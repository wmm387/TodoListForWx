<!--pages/test/test.wxml-->
<view class='container'>
  <view bindtap="box" class='box'>
    <view
      disable-scroll='true'
      wx:for="{{content}}"
      bindtouchmove="move"
      bindtouchstart="movestart"
      bindtouchend="moveend"
      data-index="{{item.id}}"
      data-main="{{mainx}}"
      class="main {{mainx == item.id ? 'mainmove' : 'mainend'}}"
      style="left:{{start.x}}px; top:{{start.y}}px">
      {{item.content}}
    </view>
  </view>
</view>